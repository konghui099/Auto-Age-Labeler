{
  "AWSEBDockerrunVersion":2,
  "containerDefinitions": [
    {
      "name":"api",
      "image":"rajs966/auto-age-api",
      "hostname":"api",
      "portMappings":[
        {
          "containerPort":5000
        }
      ],
      "essential":false,
      "memory":128
    },
    {
      "name":"client",
      "image":"rajs966/auto-age-client",
      "hostname":"client",
      "essential":false,
      "memory":128
    },
    {
      "name":"nginx",
      "image":"rajs966/auto-age-nginx",
      "hostname":"nginx",
      "portMappings":[
        {
          "hostPort":80,
          "containerPort":80
        }
      ],
      "essential":true,
      "memory":128,
      "links":["api","client"]
    }
  ]
}
